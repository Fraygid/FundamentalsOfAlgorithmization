rate=int(input('–í–≤–µ–¥–∏—Ç–µ —á–∏—Å–ª–æ–≤–æ–π —Ä–µ–π—Ç–∏–Ω–≥(1-10): '))

match rate:
    case 1|2|3:
        print('–ü–ª–æ—Ö–æ–π –ø—Ä–æ–¥—É–∫—Ç ‚ùå –ù–µ —Ä–µ–∫–æ–º–µ–Ω–¥—É—é')
    case 4|5|6:
        print('–°—Ä–µ–¥–Ω–∏–π –ø—Ä–æ–¥—É–∫—Ç üôÅ –ú–æ–∂–Ω–æ –ø–æ–ø—Ä–æ–±–æ–≤–∞—Ç—å')
    case 7|8|9:
        print('–û—Ç–ª–∏—á–Ω—ã–π –ø—Ä–æ–¥—É–∫—Ç üåü –ù–∞—Å—Ç–æ—è—Ç–µ–ª—å–Ω–æ —Ä–µ–∫–æ–º–µ–Ω–¥—É—é')
    case 10:
        print('–í–æ—Å—Ö–µ—Ç–∏—Ç–µ–ª—å–Ω—ã–π –ø—Ä–æ–¥—É–∫—Ç üíØ –û–±–µ–∑–∞—Ç–µ–ª—å–Ω–æ –ø–æ–ø—Ä–æ–±—É–π—Ç–µ')
    case _:
        print('–û—à–∏–±–∫–∞ –≤–≤–æ–¥–∞')
